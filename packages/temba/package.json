{
  "name": "temba",
  "version": "0.62.0",
  "description": "Create a simple REST API with zero coding in less than 30 seconds (seriously).",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc && tsc-alias && cp ../../README.md .",
    "test": "vitest run",
    "test:mongodb": "TEMBA_MONGODB_BASE_URI=mongodb://localhost:27017 vitest run",
    "test:custom-server": "TEMBA_CUSTOM_SERVER=true vitest run",
    "test:watch": "vitest --watch",
    "lint": "eslint . && tsc --noEmit",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "update": "npx -y jelmerro/nus"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bouwe77/temba.git"
  },
  "homepage": "https://github.com/bouwe77/temba#readme",
  "author": "Bouwe (https://bouwe.io)",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/qs": "^6.14.0",
    "@types/etag": "1.8.4",
    "@types/indefinite": "2.3.4",
    "@types/morgan": "1.9.10",
    "@types/supertest": "6.0.3",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.8.1",
    "supertest": "7.2.2",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@rakered/mongo": "1.6.0",
    "ajv": "8.17.1",
    "deepmerge": "4.3.1",
    "etag": "1.8.1",
    "indefinite": "2.5.2",
    "lowdb": "7.0.1",
    "mime": "4.1.0",
    "morgan": "1.10.1",
    "openapi3-ts": "4.5.0",
    "qs": "^6.14.0",
    "ws": "8.19.0"
  }
}
