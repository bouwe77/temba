{
  "name": "temba",
  "version": "0.46.4",
  "description": "Create a simple REST API with zero coding in less than 30 seconds (seriously).",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc-alias && cp package.json ../../README.md ./dist/src",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint . && npx tsc",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "update": "npx -y jelmerro/nus"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bouwe77/temba.git"
  },
  "homepage": "https://github.com/bouwe77/temba#readme",
  "author": "Bouwe (https://bouwe.io)",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/etag": "1.8.4",
    "@types/indefinite": "2.3.4",
    "@types/morgan": "1.9.10",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "prettier": "3.6.2",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "4.0.8"
  },
  "dependencies": {
    "@rakered/mongo": "1.6.0",
    "ajv": "8.17.1",
    "connect-pause": "0.1.0",
    "cors": "2.8.5",
    "deepmerge": "4.3.1",
    "dotenv": "17.2.3",
    "etag": "1.8.1",
    "indefinite": "2.5.2",
    "lowdb": "7.0.1",
    "mime": "4.1.0",
    "morgan": "1.10.1",
    "openapi3-ts": "4.5.0"
  }
